{
    "name": "pisystems/twig-spreadsheet-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle integrates PhpSpreadsheet into Symfony using Twig, upgraded for Symfony 5.3 / Twig 3 (PDF Support dropped).",
    "keywords": ["excel","phpexcel","spreadsheet","phpspreadsheet","twig","symfony"],
    "homepage": "https://github.com/PISystems/TwigSpreadsheetBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mewes Kochheim",
            "email": "mewes@kochheim.de"
        },
        {
            "name": "PISystems",
            "email": "noreply@iboss.nl"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/MewesK/TwigSpreadsheetBundle/graphs/contributors"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/PISystems/TwigSpreadsheetBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.3",
        "twig/twig": "^3.3",
        "phpoffice/phpspreadsheet": "~1.18"
    },
    "require-dev": {
        "symfony/symfony": "^5.3",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/phpunit-bridge": "^v5.3",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigSpreadsheetBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "MewesK\\TwigSpreadsheetBundle\\Tests\\": "tests/" }
    }
}
